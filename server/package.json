{
  "name": "@dejames-13/dej-server-starter",
  "version": "0.0.2",
  "description": "A starter template for Express Server projects, for personal use only",
  "type": "module",
  "scripts": {
    "start": "node api/index.js",
    "dev": "nodemon api/index.js -e js,ts,json",
    "lint": "eslint . "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deJames-13/dej-server-starter.git"
  },
  "imports": {
    "#middlewares": "./middlewares/index.js",
    "#middlewares/*": "./middlewares/*.js",
    "#common": "./common/index.js",
    "#common/*": "./common/*.js",
    "#constants": "./common/constants/index.js",
    "#constants/*": "./common/constants/*.js",
    "#lib": "./common/lib/index.js",
    "#lib/*": "./common/lib/*.js",
    "#utils": "./common/utils/index.js",
    "#utils/*": "./common/utils/*.js",
    "#config": "./common/config/index.js",
    "#config/*": "./common/config/*.js",
    "#features": "./features/index.js",
    "#firebase/*": "./firebase/*.js"
  },
  "dependencies": {
    "bad-words": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "change-case": "^5.4.4",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.1.0",
    "firebase-admin": "^12.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.9.15",
    "slugify": "^1.6.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@faker-js/faker": "^9.0.3",
    "@types/node": "^22.7.7",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "express",
    "starter",
    "server",
    "template"
  ],
  "author": "deJames-13 <de.james013@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deJames-13/dej-server-starter/issues"
  },
  "homepage": "https://github.com/deJames-13/dej-server-starter#readme"
}
